{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "contain",
          "-webkit-tap-highlight-color",
          "overscroll-behavior-y",
          "text-size-adjust",
          "scrollbar-width",
          "scrollbar-width"
        ]
      }
    ],
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "typescript-config/is-valid": "off"
  }
}