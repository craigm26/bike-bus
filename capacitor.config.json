{
  "appId": "app.bikebusapp",
  "appName": "BikeBusApp",
  "webDir": "build",
  "bundledWebRuntime": false,
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "@capacitor-firebase/authentication": {
      "skipNativeAuth": false,
      "providers": ["google.com", "password", "emailLink", "phone"]
    },
    "FirebaseAuthentication": {
      "skipNativeAuth": false,
      "providers": ["google.com", "password", "emailLink", "phone"]
    }
  }
}
